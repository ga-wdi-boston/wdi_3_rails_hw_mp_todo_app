<div class="byline">
<div class="button_slider">
  <%= button_to projects_url, class: "btn btn-primary", method: :get do %>
    <i class="fa fa-home fa-lg"></i> Home
  <% end %>
</div>
<div class="button_slider">
  <%= button_to projects_url, method: :get, class: "btn btn-info" do %>
    <i class="fa fa-step-backward fa-lg"></i> Back
  <% end %>
</div>


</div>
<%= bootstrap_form_for [@project] do |f| %>
    <%= render 'shared/errors', object: @project %>
    <p>
      <%= f.text_field :name, autofocus: true, label: "Project Name" %>
    </p>
    <p>
      <%= f.text_field :description %>
    </p>
    <div class="button_slider_submit">
        <%= f.submit class: "btn btn-success"%>
    </div>
<% end %>
<br>



