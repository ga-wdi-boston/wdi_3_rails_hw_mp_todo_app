<%= form_for [@project] do |f| %>
    <%= render 'shared/errors', object: @project %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, autofocus: true %>
    </p>
    <p>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </p>
    <%= f.submit %>
    <%= link_to "Cancel", projects_path %>
    <% end %>
