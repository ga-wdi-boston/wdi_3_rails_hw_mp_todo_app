<p id="notice"><%= notice %></p>

<h1><%= 'List' %>: <%= @list.name %>
<h2> Tasks to complete </h4>

 <ul>
  <% @list.tasks.each do |task| %>
  <li><%= task.name %></li>
  <% end %>
 </ul>


<%= link_to 'All Tasks', list_tasks_path(@list, @task)  %> |
<%= link_to 'New Task', new_list_task_path(@list, @task)  %> |
<%= link_to 'Edit', edit_list_path(@list) %> |
<%= link_to 'Back', lists_path %>
