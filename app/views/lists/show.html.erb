
<p id="notice"><%= notice %></p>
<h1><%= @list.name %></h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list.tasks.each do |task| %>
      <tr>
        <td><%= link_to task.name, list_task_path(@list, task) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task', new_list_task_path(@list) %>
<%= link_to 'Back', lists_path %>
