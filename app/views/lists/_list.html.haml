.container
  %table.table.table-hover.table-bordered
    %tr
      %th List Name
      %th Edit
      %th Destroy
    %tbody
      %tr
        %td 
          =link_to list.name, list
        %td 
          =link_to 'Edit', edit_list_path(list)
        %td 
          =link_to 'Destroy List', [list.project, list], method: :delete, data: { confirm: 'Are you sure?' }
