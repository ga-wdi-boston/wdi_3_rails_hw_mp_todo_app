<%= form_for [@project, @list] do |f| %>
    <%= render 'shared/errors', object: @list %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, autofocus: true %>
    </p>
    <p>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </p>
    <%= f.submit %>
    <%= link_to "Cancel", project_path(@list.project_id) %>
    <% end %>
