<%= form_for @list do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :category %>
  <%= f.text_field :category %>

  <%= f.label :part_of_project %>
  <%= f.text_field :part_of_project %>

  <%= f.label :progress %>
  <%= f.text_field :progress %>

  <%= f.submit %>
<% end %>

<%= link_to "Back to All Lists", index_list_path %>
