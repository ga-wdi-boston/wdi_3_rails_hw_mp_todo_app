<h1>New Task</h1>
<%= bootstrap_form_for [@list, @task] do |f| %>

    <%= render 'shared/errors', object: @task %>
    <p>
      <%= f.text_field :name, autofocus: true, label: "Task Name" %>
    </p>
    <p>
      <%= f.check_box( "completed", {}, "true", "false") %>
    </p>
    <div class="button_slider_submit">
        <%= f.submit class: "btn btn-success"%>
    </div>
<% end %>
<br>

